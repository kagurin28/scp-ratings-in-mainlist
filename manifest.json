{
	"manifest_version": 2,
	"name": "SCP Ratings in Mainlist",
	"version": "2.0",
	"author": "Oliver Robey",
	
	"description": "Shows the rating of the SCP in the mainlist (SCP Series, SCP Series 2, etc).",
	
	"icons": {
		"48": "icon-48.png"
	},
	
	"content_scripts": [
		{
			"matches": [
				"https://scp-wiki.wikidot.com/scp-series*",
				"https://scp-wiki.wikidot.com/joke-scps*",
				"https://scp-wiki.wikidot.com/scp-ex",
				"https://scp-wiki.wikidot.com/explained-scps-tales-edition",
				"https://scp-wiki.wikidot.com/scp-001"
			],
			
			"js": ["scp-ratings-in-mainlist.js"]
		}
	]
}
